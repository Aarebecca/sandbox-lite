{
  "name": "sandbox-lite",
  "version": "1.0.0-alpha.0",
  "description": "A simple node code execution sandbox, supporting TypeScript and basic security checks",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "src",
    "lib",
    "esm"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "rm -rf lib && tsc --module commonjs --outDir lib",
    "build:esm": "rm -rf esm && tsc --module esnext --outDir esm",
    "test": "npm run build && bash ./scripts/test.sh",
    "prepublishOnly": "npm run test"
  },
  "keywords": [
    "node",
    "sandbox",
    "vm"
  ],
  "author": "https://github.com/Aarebecca",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Aarebecca/sandbox-lite"
  },
  "dependencies": {
    "@eslint/js": "^9.26.0",
    "eslint": "^9.26.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^15.15.0",
    "minimatch": "^10.0.1",
    "sucrase": "^3.35.0"
  },
  "devDependencies": {
    "@antv/g2-ssr": "^0.0.8",
    "@antv/g6": "^5.0.46",
    "@antv/g6-ssr": "^0.0.7",
    "@types/babel__core": "^7.20.5",
    "@types/node": "^22.15.18",
    "tsx": "^4.19.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
